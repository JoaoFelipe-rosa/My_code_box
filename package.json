{
  "name": "my_code_box",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "run:all": "concurrently \"yarn dev\" \"yarn start:backend\" \"yarn start:studio\"",
    "clean": "chmod +x Scripts/clean.sh && sh Scripts/clean.sh && yarn",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx,.jsx,.js",
    "preview": "vite preview",
    "start:backend": "node --loader ts-node/esm Api/Api.ts",
    "start:frontend": "vite",
    "updateApi": "npx prisma db push",
    "start:studio": "npx prisma studio"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@prisma/client": "^5.18.0",
    "axios": "^1.6.5",
    "caniuse-lite": "^1.0.30001651",
    "cors": "^2.8.5",
    "craco-alias": "^3.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.19.2",
    "formik": "^2.4.6",
    "node": "^22.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.21.1",
    "ts-node-dev": "^2.0.0",
    "yarn": "^1.22.19",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "eslint": "^8.43.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^4.3.9"
  },
  "main": "index.js",
  "repository": "https://github.com/JoaoFelipe-rosa/My_code_box.git",
  "author": "JoaoFelipe-rosa <joaofelipedarosa@gmail.com>",
  "license": "MIT"
}
